{
  "name": "parmenion-library",
  "version": "0.0.1",
  "description": "A boilerplate for running a Webpack workflow in Node express",
  "main": "server.js",
  "author": "Ben Dunn",
  "license": "MIT",
  "private": true,
  "bugs": {
    "url": "https://github.com/parmenionux/parmenion-library/issues"
  },
  "homepage": "https://github.com/parmenion/parmenion-library",
  "scripts": {
    "test": "npm run lint",
    "start": "node server",
    "build": "rimraf dist && cross-env NODE_ENV=production webpack --config ./webpack.production.config.js --progress --profile --colors",
    "lint": "eslint --ext .js,.jsx .",
    "styleguide-server": "styleguidist server",
    "styleguide-build": "styleguidist build",
    "styleguide-publish": "npm run styleguide-build && gh-pages -d styleguide"
  },
  "dependencies": {
    "aphrodite": "^1.1.0",
    "babel-cli": "^6.4.0",
    "fuzzaldrin": "^2.1.0",
    "react": "^0.14.3",
    "react-dom": "^0.14.3",
    "react-redux": "^4.3.0",
    "react-router": "^2.0.0",
    "react-router-redux": "^4.0.7",
    "redux": "^3.2.1",
    "redux-devtools": "^3.1.0",
    "redux-devtools-dock-monitor": "^1.0.1",
    "redux-devtools-log-monitor": "^1.0.4",
    "styled-components": "^1.1.3",
    "tinycolor2": "^1.4.1"
  },
  "devDependencies": {
    "autoprefixer": "^6.0.3",
    "babel-eslint": "^7.1.1",
    "babel-jscs": "^2.0.5",
    "babel-loader": "^6.2.3",
    "babel-plugin-styled-components": "0.0.2",
    "babel-plugin-styled-components-selector": "^1.0.20",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-react-hmre": "^1.1.0",
    "babel-preset-stage-0": "^6.3.13",
    "chai": "^3.2.0",
    "cross-env": "^1.0.7",
    "css-loader": "^0.19.0",
    "eslint": "^3.13.0",
    "eslint-config-airbnb": "^14.0.0",
    "eslint-import-resolver-webpack": "^0.8.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^3.0.2",
    "eslint-plugin-react": "^6.9.0",
    "express": "^4.13.3",
    "extract-text-webpack-plugin": "^1.0.1",
    "gh-pages": "^0.12.0",
    "html-webpack-plugin": "^1.6.1",
    "jscs": "^2.1.1",
    "jsdom": "^6.5.1",
    "json-loader": "^0.5.3",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "mocha": "^2.3.3",
    "mocha-jsdom": "^1.0.0",
    "postcss-loader": "^0.6.0",
    "react-addons-test-utils": "^0.14.3",
    "react-styleguidist": "github:madjam002/react-styleguidist#multiple-components",
    "react-transform-hmr": "^1.0.0",
    "rimraf": "^2.4.3",
    "sinon": "^1.16.1",
    "sinon-chai": "^2.8.0",
    "stats-webpack-plugin": "^0.2.1",
    "style-loader": "^0.12.4",
    "webpack": "^1.12.2",
    "webpack-dev-middleware": "^1.2.0",
    "webpack-hot-middleware": "^2.2.0"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": [
      "airbnb"
    ],
    "env": {
      "browser": true
    },
    "settings": {
      "import/resolver": "webpack"
    },
    "rules": {
      "react/jsx-filename-extension": 0,
      "react/jsx-no-bind": 0,
      "react/prop-types": 0,
      "react/require-default-props": 0,
      "react/no-find-dom-node": 0,
      "react/no-unescaped-entities": 0,
      "import/prefer-default-export": 0,
      "import/no-named-as-default": 0,
      "max-len": 0,
      "linebreak-style": 0,
      "no-unused-vars": [
        2,
        {
          "args": "none"
        }
      ],
      "no-confusing-arrow": 0,
      "func-names": 0
    }
  }
}
